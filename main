  title = TF-A Tests patches
  description = Main dashboard for the Trusted Firmware-A Tests project
  foreach = project:TF-A/tf-a-tests

[section "Open Published Patches"]
  query = is:open -is:wip

[section "Open WIP Patches"]
  query = is:open is:wip

[section "Recently Abandoned"]
  query = is:abandoned -age:1w

[section "Recently Merged"]
  query = is:merged -age:1w

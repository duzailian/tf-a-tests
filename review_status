[dashboard]
  title = TF-A-Tests Patch Review Status
  description = Review the status of pending patches and reviews
  foreach = project:TF-A/tf-a-tests

[section "+2ed but pending merge"]
  query = is:open label:Code-Review=2

[section ">7 days"]
  query = is:open -is:draft age:7d -age:14d -label:Code-Review=2

[section ">14 days"]
  query = is:open -is:draft age:14d -age:21d -label:Code-Review=2

[section ">21 days"]
  query = is:open -is:draft age:21d -label:Code-Review=2
